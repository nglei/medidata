<body>
    <div class="page-wrapper bg-blue p-t-100 p-b-100 font-robo">
        <div class="wrapper wrapper--w680">
            <div class="card card-1">
                <div class="login-form-head">
                        <h4>Doctor Sign up</h4>
                        <p>Hello there, Sign up and Join with Us</p>
                    </div>
                <div class="card-body">
                    <h2 class="title">Registration Info</h2>
                    <form action="" method="POST" id="register" onsubmit="return validation()">
					<div class="inputDiv">
                        <div class="input-group">
                            <input class="input--style-1" type="text" id="username" name="username" placeholder="USERNAME">
                        </div>
						<div class="error">
							<span id="errorUsername" class="error"></span>
						</div>
					</div>

					<div class="inputDiv">
						<div class="input-group">
                            <input class="input--style-1" type="email" id="email" name="email" placeholder="EMAIL ADDRESS">
                        </div>
						<div class="error">
							<span id="errorEmail" class="error"></span>
						</div>
					</div>

					<div class="inputDiv">
						<div class="input-group">
                            <input class="input--style-1" type="password" id="password" name="password" placeholder="PASSWORD">
                        </div>
						<div class="error">
							<span id="errorPass" class="error"></span>
						</div>
					</div>

					<div class="inputDiv">
						<div class="input-group">
                            <input class="input--style-1" type="password" id="cpassword" name="cPassword" placeholder="CONFIRM PASSWORD">
                        </div>
						<div class="error">
							<span id="errorCPass" class="error"></span>
						</div>
					</div>
          <div class="inputDiv">
            <div class="input-group">
              <input class="input--style-1" type="text" id="name" name="name" placeholder="FULL NAME">
            </div>
            <div class="error">
              <span id="errorName" class="error"></span>
            </div>
          </div>
          <div class="inputDiv">
            <div class="input-group">
              <input class="input--style-1" type="text" id="hospital" name="hospital" placeholder="Hospital Name">
            </div>
            <div class="error">
              <span id="errorName" class="error"></span>
            </div>
          </div>

                        <div class="p-t-20">
                            <button class="btn btn--radius btn--green" type="submit" value="submit" form="register">Submit</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

